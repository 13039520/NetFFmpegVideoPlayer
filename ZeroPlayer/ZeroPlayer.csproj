		</None>
		<None Include="..\FFmpeg\x86\swresample-4.dll" Link="FFmpeg\x86\swresample-4.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Include="..\FFmpeg\x86\swscale-7.dll" Link="FFmpeg\x86\swscale-7.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
	  <PackageReference Include="FFmpeg.AutoGen">
	    <Version>6.0.0</Version>
	  </PackageReference>
	  <PackageReference Include="FFmpeg.AutoGen.Abstractions">
	    <Version>6.0.0</Version>
	  </PackageReference>
	  <PackageReference Include="FFmpeg.AutoGen.Bindings.DynamicallyLinked">
	    <Version>6.0.0</Version>
	  </PackageReference>
	  <PackageReference Include="FFmpeg.AutoGen.Bindings.DynamicallyLoaded">
	    <Version>6.0.0</Version>
	  </PackageReference>
	  <PackageReference Include="FFmpeg.AutoGen.Bindings.StaticallyLinked">
	    <Version>6.0.0</Version>
	  </PackageReference>
	  <PackageReference Include="SkiaSharp">
	    <Version>2.88.5</Version>
	  </PackageReference>
	</ItemGroup>

</Project>
