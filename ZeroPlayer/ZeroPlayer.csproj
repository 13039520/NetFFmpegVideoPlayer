<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>
	</PropertyGroup>
	<ItemGroup>
		<Folder Include="FFmpeg" />
	</ItemGroup>
	<ItemGroup>
		<None Include="..\FFmpeg\x64\avcodec-60.dll" Link="FFmpeg\x64\avcodec-60.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Include="..\FFmpeg\x64\avdevice-60.dll" Link="FFmpeg\x64\avdevice-60.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Include="..\FFmpeg\x64\avfilter-9.dll" Link="FFmpeg\x64\avfilter-9.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Include="..\FFmpeg\x64\avformat-60.dll" Link="FFmpeg\x64\avformat-60.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Include="..\FFmpeg\x64\avutil-58.dll" Link="FFmpeg\x64\avutil-58.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Include="..\FFmpeg\x64\postproc-57.dll" Link="FFmpeg\x64\postproc-57.dll">
